<div class="row">
  <div class="page-header">
    <h1><%= @repository.name %></h1>
  </div>

  <table class="table table-hover">
    <thead>
    <tr>
      <th>Users</th>
    </tr>
    </thead>
    <tbody>
    <%= content_tag_for(:tr, @repository.users {|user| user.id}) do |user| %>
      <td><%= link_to user.account_name, user_path(user.id) %></td>
    <% end %>
    </tbody>
  </table>
  <%= link_to 'Back', 'javascript:history.go(-1);', class: 'btn btn-default' %>


</div>
